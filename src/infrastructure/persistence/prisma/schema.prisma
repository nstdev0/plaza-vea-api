generator client {
  provider = "prisma-client"
  output   = "../generated/prisma"
}

datasource db {
  provider = "postgresql"
}

model Product {
  skuId      String   @id
  name       String
  searchName String
  ean        String?  @unique
  price      BigInt
  imageUrl   String?
  brand      String?
  category   String[]
  rawJson    Json
  createdAt  DateTime @default(now())
  updatedAt  DateTime @updatedAt
}
